{
    "name": "asifulmamun/dianasms-esms",
    "description": "Laravel package for ESMS (login.esms.com.bd) to send SMS (single/multiple, schedule).",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sms",
        "esms",
        "bangladesh",
        "bd",
        "api"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Asifulmamun\\DianasmsEsms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asifulmamun\\DianasmsEsms\\EsmsServiceProvider"
            ],
            "aliases": {
                "Esms": "Asifulmamun\\DianasmsEsms\\Facades\\Esms"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}